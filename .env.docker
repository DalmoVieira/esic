# Docker environment variables
# Copy this file to .env and configure your values

# PostgreSQL Configuration
POSTGRES_USER=esic
POSTGRES_PASSWORD=esic_password
POSTGRES_DB=esic_db
POSTGRES_PORT=5432

# Application Configuration
APP_PORT=3001
NODE_ENV=production

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this
JWT_EXPIRES_IN=7d

# Database URL (for Prisma)
DATABASE_URL=postgresql://esic:esic_password@postgres:5432/esic_db?schema=public
